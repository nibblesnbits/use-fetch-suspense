{
  "name": "use-fetch",
  "version": "1.0.0",
  "description": "React hooks for data fetching with Suspense",
  "author": "nibblesnbits",
  "license": "MIT",
  "repository": "nibblesnbits/use-fetch",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "test": "cross-env CI=1 react-scripts-ts test --env=jsdom",
    "test:watch": "react-scripts-ts test --env=jsdom",
